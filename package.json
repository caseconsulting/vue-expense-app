{
  "name": "vue-expense-app",
  "version": "4.20.0",
  "versionName": "Skunk",
  "releaseDate": "2025-10-27",
  "releaseNotes": "https://3.basecamp.com/3097063/buckets/4708396/documents/9208241297",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider && vite build",
    "clean": "rm -rf dist tests_output",
    "predeploy:dev": "npm run download:dev:env",
    "deploy:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider && npm run build && node deploy.js -dev",
    "postdeploy:dev": "npm run download:local:env",
    "predeploy:prod": "cross-env AWS_PROFILE=prod && npm run download:prod:env",
    "deploy:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider && npm run build  && node deploy.js -prod",
    "postdeploy:prod": "cross-env AWS_PROFILE=default && npm run download:local:env",
    "predeploy:test": "npm run download:test:env",
    "deploy:test": "cross-env NODE_OPTIONS=--openssl-legacy-provider && npm run build && node deploy.js -test",
    "postdeploy:test": "npm run download:local:env",
    "predeploy:sandbox": "npm run download:sandbox:env",
    "deploy:sandbox": "cross-env NODE_OPTIONS=--openssl-legacy-provider && npm run build && node deploy.js -sandbox",
    "postdeploy:sandbox": "npm run download:local:env",
    "download:sandbox:env": "aws s3 cp s3://case-expense-app-resources-sandbox/.env .env",
    "download:dev:env": "aws s3 cp s3://case-expense-app-resources-dev/.env .env",
    "download:local:env": "aws s3 cp s3://case-expense-app-resources-local/.env .env",
    "download:prod:env": "aws s3 cp s3://case-consulting-expense-app-resources-prod/.env .env --profile prod",
    "download:test:env": "aws s3 cp s3://case-expense-app-resources-test/.env .env",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "lint": "npm run eslint && npm run stylelint",
    "precommit": "npm run validate && npm run eslint && npm run stylelint && npm run test",
    "reinstall": "echo Removing old files... && rm -rf node_modules package-lock.json && echo Done. && echo && npm install && npm ci",
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider && vite dev --port 8080 --host",
    "start": "npm run serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.5.0"
  },
  "dependencies": {
    "@date-io/dayjs": "3.2.0",
    "@mdi/font": "7.4.47",
    "auth0-js": "9.29.0",
    "axios": "1.12.2",
    "chart.js": "4.5.0",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.18",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "mobile-detect": "1.4.5",
    "papaparse": "5.5.3",
    "patternomaly": "1.3.2",
    "vue": "3.5.22",
    "vue-chartjs": "5.3.2",
    "vue-confetti-explosion": "1.0.2",
    "vue-loader": "17.4.2",
    "vue-router": "4.5.1",
    "vue-router-multiguard": "1.0.3",
    "vue-the-mask": "0.11.1",
    "vuetify": "3.10.5",
    "vuewordcloud": "19.0.0",
    "vuex": "4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/eslint-parser": "7.28.4",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/compiler-sfc": "3.5.22",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "@vue/vue3-jest": "29.2.6",
    "babel-jest": "29.7.0",
    "eslint": "9.37.0",
    "eslint-plugin-vue": "10.5.0",
    "cross-env": "^10.1.0",
    "eslint-plugin-vuetify": "2.5.3",
    "postcss": "8.5.6",
    "postcss-html": "1.8.0",
    "postcss-loader": "8.2.0",
    "precommit-hook": "3.0.0",
    "prettier": "3.6.2",
    "sass": "1.93.2",
    "stylelint": "16.25.0",
    "stylelint-config-recommended-scss": "16.0.2",
    "stylelint-config-recommended-vue": "1.6.1",
    "vite": "7.1.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "eslint",
    "stylelint"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}

{
  "name": "vue-expense-app",
  "version": "4.2.0",
  "private": true,
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "clean": "rm -rf dist tests_output",
    "cli": "vue-cli-service",
    "predeploy:dev": "npm run download:dev:env",
    "deploy:dev": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build && grunt deploy:dev",
    "postdeploy:dev": "npm run download:local:env",
    "predeploy:prod": "export AWS_PROFILE=prod && npm run download:prod:env",
    "deploy:prod": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build && grunt deploy:prod",
    "postdeploy:prod": "export AWS_PROFILE=default && npm run download:local:env",
    "predeploy:test": "npm run download:test:env",
    "deploy:test": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build && grunt deploy:test",
    "postdeploy:test": "npm run download:local:env",
    "download:dev:env": "aws s3 cp s3://case-expense-app-resources-dev/.env .env",
    "download:local:env": "aws s3 cp s3://case-expense-app-resources-local/.env .env",
    "download:prod:env": "aws s3 cp s3://case-consulting-expense-app-resources-prod/.env .env --profile prod",
    "download:test:env": "aws s3 cp s3://case-expense-app-resources-test/.env .env",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "inspect": "vue-cli-service inspect > output.log",
    "lint": "npm run eslint && npm run stylelint",
    "precommit": "npm run validate && npm run eslint && npm run stylelint && npm run test",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "start": "npm run serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@date-io/dayjs": "3.0.0",
    "@mdi/font": "7.4.47",
    "auth0-js": "9.24.1",
    "axios": "1.6.7",
    "chart.js": "4.4.2",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.10",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "mobile-detect": "1.4.5",
    "patternomaly": "1.3.2",
    "vue": "3.4.21",
    "vue-chartjs": "5.3.0",
    "vue-loader": "17.4.2",
    "vue-router": "4.3.0",
    "vue-router-multiguard": "1.0.3",
    "vue-the-mask": "0.11.1",
    "vuetify": "3.5.8",
    "vuewordcloud": "19.0.0",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@babel/eslint-parser": "7.23.10",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-unit-jest": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/compiler-sfc": "3.4.21",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/test-utils": "2.4.4",
    "babel-jest": "29.7.0",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.22.0",
    "eslint-plugin-vuetify": "2.1.1",
    "grunt": "1.6.1",
    "grunt-shell": "4.0.0",
    "postcss": "8.4.35",
    "postcss-html": "1.6.0",
    "postcss-loader": "8.1.1",
    "precommit-hook": "3.0.0",
    "prettier": "3.2.5",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "stylelint": "16.2.1",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "vue-template-compiler": "2.7.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "eslint",
    "stylelint"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}

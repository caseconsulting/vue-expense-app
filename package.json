{
  "name": "vue-expense-app",
  "version": "3.6.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "cli": "vue-cli-service",
    "predeploy:dev": "npm run download:dev:env",
    "deploy:dev": "vue-cli-service build && grunt deploy:dev",
    "postdeploy:dev": "npm run download:local:env",
    "predeploy:prod": "export AWS_PROFILE=prod && npm run download:prod:env",
    "deploy:prod": "vue-cli-service build && grunt deploy:prod",
    "postdeploy:prod": "export AWS_PROFILE=default && npm run download:local:env",
    "predeploy:test": "npm run download:test:env",
    "deploy:test": "vue-cli-service build && grunt deploy:test",
    "postdeploy:test": "npm run download:local:env",
    "download:dev:env": "aws s3 cp s3://case-expense-app-resources-dev/.env .env",
    "download:local:env": "aws s3 cp s3://case-expense-app-resources-local/.env .env",
    "download:prod:env": "aws s3 cp s3://case-consulting-expense-app-resources-prod/.env .env --profile prod",
    "download:test:env": "aws s3 cp s3://case-expense-app-resources-test/.env .env",
    "e2e": "npm run test:e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "inspect": "vue-cli-service inspect > output.log",
    "lint": "npm run eslint",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "node ./node_modules/nightwatch/bin/nightwatch -e default unitTests --suiteRetries 2",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "auth0-js": "9.19.0",
    "axios": "0.27.2",
    "chart.js": "2.9.4",
    "crypto-js": "4.1.1",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "@mdi/font": "^6.7.96",
    "mobile-detect": "1.4.5",
    "moment-timezone": "0.5.34",
    "patternomaly": "1.3.2",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-awesome": "4.5.0",
    "vue-chartjs": "3.5.1",
    "vue-router": "3.5.3",
    "vue-router-multiguard": "1.0.3",
    "vue-the-mask": "0.11.1",
    "vue2-filters": "0.14.0",
    "vuetify": "2.6.6",
    "vuewordcloud": "18.7.12",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@vue/cli-plugin-babel": "4.5.17",
    "@vue/cli-plugin-e2e-cypress": "4.5.17",
    "@vue/cli-plugin-unit-jest": "4.5.17",
    "@vue/cli-service": "4.5.17",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/test-utils": "1.3.0",
    "babel-jest": "27.5.1",
    "chokidar": "3.5.3",
    "chromedriver": "102.0.0",
    "eslint": "8.17.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "fibers": "5.0.1",
    "geckodriver": "3.0.1",
    "grunt": "1.5.3",
    "grunt-shell": "4.0.0",
    "nightwatch": "1.7.13",
    "postcss": "8.4.12",
    "postcss-html": "1.4.1",
    "postcss-loader": "4.3.0",
    "precommit-hook": "3.0.0",
    "prettier": "2.6.2",
    "raw-loader": "4.0.2",
    "sass": "1.51.0",
    "sass-loader": "10.2.1",
    "stylelint": "14.8.1",
    "stylelint-config-recommended-scss": "6.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylus": "0.57.0",
    "stylus-loader": "3.0.2",
    "vue-template-compiler": "2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "lint",
    "stylelint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}

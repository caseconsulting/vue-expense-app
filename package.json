{
  "name": "vue-expense-app",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "cli": "vue-cli-service",
    "deploy:dev": "vue-cli-service build && grunt deploy:dev",
    "deploy:prod": "vue-cli-service build && grunt deploy:prod",
    "deploy:test": "vue-cli-service build && grunt deploy:test",
    "e2e": "npm run test:e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "eslint": "npm run lint",
    "inspect": "vue-cli-service inspect > output.log",
    "lint": "vue-cli-service lint",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "stylelint": "stylelint 'src/**/*.{css,scss,vue}'",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "accounting-js": "^1.1.1",
    "auth0-js": "^9.11.2",
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.0.0-rc.28",
    "chart.js": "^2.8.0",
    "chartjs": "^0.3.24",
    "core-js": "^2.6.9",
    "crypto-js": "^3.1.9-1",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "mobile-detect": "^1.4.3",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "patternomaly": "^1.3.2",
    "vue": "^2.6.10",
    "vue-awesome": "^3.5.4",
    "vue-chartjs": "^3.4.2",
    "vue-router": "^3.1.3",
    "vue-router-multiguard": "^1.0.3",
    "vue2-filters": "^0.6.1",
    "vuejs-datepicker": "^1.6.2",
    "vuetify": "^1.5.16",
    "vuex": "^3.1.1",
    "xml2js": "^0.4.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "grunt": "^1.0.4",
    "grunt-aws-s3": "^2.0.1",
    "node-sass": "^4.12.0",
    "precommit-hook": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.3.1",
    "stylelint": "^10.1.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.10.0",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "lint",
    "stylelint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}

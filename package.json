{
  "name": "vue-expense-app",
  "version": "3.5.2",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "cli": "vue-cli-service",
    "predeploy:dev": "npm run download:dev:env",
    "deploy:dev": "vue-cli-service build && grunt deploy:dev",
    "postdeploy:dev": "npm run download:local:env",
    "predeploy:prod": "export AWS_PROFILE=prod && npm run download:prod:env",
    "deploy:prod": "vue-cli-service build && grunt deploy:prod",
    "postdeploy:prod": "export AWS_PROFILE=default && npm run download:local:env",
    "predeploy:test": "npm run download:test:env",
    "deploy:test": "vue-cli-service build && grunt deploy:test",
    "postdeploy:test": "npm run download:local:env",
    "download:dev:env": "aws s3 cp s3://case-consulting-expense-app-resources-dev/.env .env",
    "download:local:env": "aws s3 cp s3://case-expense-app/.env .env",
    "download:prod:env": "aws s3 cp s3://case-consulting-expense-app-resources-prod/.env .env --profile prod",
    "download:test:env": "aws s3 cp s3://case-consulting-expense-app-resources-test/.env .env",
    "e2e": "npm run test:e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "eslint": "npm run lint",
    "inspect": "vue-cli-service inspect > output.log",
    "lint": "vue-cli-service lint",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "node ./node_modules/nightwatch/bin/nightwatch -e default unitTests --suiteRetries 2",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "27.1.0",
    "@ckeditor/ckeditor5-basic-styles": "27.1.0",
    "@ckeditor/ckeditor5-block-quote": "27.1.0",
    "@ckeditor/ckeditor5-dev-utils": "27.1.1",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "27.1.1",
    "@ckeditor/ckeditor5-editor-classic": "27.1.0",
    "@ckeditor/ckeditor5-essentials": "27.1.0",
    "@ckeditor/ckeditor5-font": "27.1.0",
    "@ckeditor/ckeditor5-heading": "27.1.0",
    "@ckeditor/ckeditor5-image": "27.1.0",
    "@ckeditor/ckeditor5-link": "27.1.0",
    "@ckeditor/ckeditor5-list": "27.1.0",
    "@ckeditor/ckeditor5-markdown-gfm": "27.1.0",
    "@ckeditor/ckeditor5-media-embed": "27.1.0",
    "@ckeditor/ckeditor5-theme-lark": "27.1.0",
    "@ckeditor/ckeditor5-upload": "27.1.0",
    "@ckeditor/ckeditor5-vue2": "1.0.5",
    "auth0-js": "9.19.0",
    "axios": "0.26.0",
    "chart.js": "2.9.4",
    "crypto-js": "4.1.1",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "mobile-detect": "1.4.5",
    "moment-timezone": "0.5.34",
    "patternomaly": "1.3.2",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-awesome": "4.5.0",
    "vue-chartjs": "3.5.1",
    "vue-router": "3.5.3",
    "vue-router-multiguard": "1.0.3",
    "vue-the-mask": "0.11.1",
    "vue2-filters": "0.14.0",
    "vuetify": "2.4.9",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-e2e-cypress": "4.5.15",
    "@vue/cli-plugin-eslint": "4.5.15",
    "@vue/cli-plugin-unit-jest": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.3.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "chokidar": "3.5.3",
    "chromedriver": "98.0.1",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "7.20.0",
    "geckodriver": "3.0.1",
    "grunt": "1.4.1",
    "grunt-aws-s3": "2.0.2",
    "nightwatch": "1.7.2",
    "node-sass": "6.0.1",
    "postcss-loader": "3.0.0",
    "precommit-hook": "3.0.0",
    "prettier": "2.5.1",
    "raw-loader": "4.0.2",
    "sass-loader": "10.2.1",
    "stylelint": "13.13.1",
    "stylelint-checkstyle-formatter": "0.1.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-processor-html": "1.0.0",
    "stylelint-scss": "3.21.0",
    "stylus": "0.56.0",
    "stylus-loader": "3.0.2",
    "vue-template-compiler": "2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "lint",
    "stylelint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}

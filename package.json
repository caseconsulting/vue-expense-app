{
  "name": "vue-expense-app",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "cli": "vue-cli-service",
    "predeploy:dev": "npm run download:dev:env",
    "deploy:dev": "vue-cli-service build && grunt deploy:dev",
    "postdeploy:dev": "npm run download:local:env",
    "predeploy:prod": "npm run download:prod:env",
    "deploy:prod": "vue-cli-service build && grunt deploy:prod",
    "postdeploy:prod": "npm run download:local:env",
    "predeploy:test": "npm run download:test:env",
    "deploy:test": "vue-cli-service build && grunt deploy:test",
    "postdeploy:test": "npm run download:local:env",
    "download:dev:env": "aws s3 cp s3://case-consulting-expense-app-resources-dev/.env .env",
    "download:local:env": "aws s3 cp s3://case-expense-app/.env .env",
    "download:prod:env": "aws s3 cp s3://case-consulting-expense-app-resources-prod/.env .env --profile prod",
    "download:test:env": "aws s3 cp s3://case-consulting-expense-app-resources-test/.env .env",
    "e2e": "npm run test:e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "eslint": "npm run lint",
    "inspect": "vue-cli-service inspect > output.log",
    "lint": "vue-cli-service lint",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^21.0.0",
    "@ckeditor/ckeditor5-build-classic": "^20.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^23.1.1",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^23.1.1",
    "@ckeditor/ckeditor5-editor-classic": "^21.0.0",
    "@ckeditor/ckeditor5-essentials": "^21.0.0",
    "@ckeditor/ckeditor5-link": "^21.0.0",
    "@ckeditor/ckeditor5-paragraph": "^21.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^21.0.0",
    "@ckeditor/ckeditor5-upload": "^21.0.0",
    "@ckeditor/ckeditor5-vue": "^1.0.1",
    "accounting-js": "^1.1.1",
    "auth0-js": "^9.13.4",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "chart.js": "^2.9.3",
    "chartjs": "^0.3.24",
    "core-js": "^2.6.11",
    "crypto-js": "^3.3.0",
    "file-saver": "^2.0.2",
    "jquery": "^3.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "mobile-detect": "^1.4.4",
    "moment": "^2.27.0",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.31",
    "patternomaly": "^1.3.2",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-awesome": "^4.0.2",
    "vue-chartjs": "^3.5.0",
    "vue-router": "^3.3.4",
    "vue-router-multiguard": "^1.0.3",
    "vue2-filters": "^0.11.0",
    "vuejs-datepicker": "^1.6.2",
    "vuetify": "^2.3.4",
    "vuex": "^3.5.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-e2e-cypress": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "grunt": "^1.2.1",
    "grunt-aws-s3": "^2.0.2",
    "node-sass": "^4.14.1",
    "precommit-hook": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^7.3.1",
    "stylelint": "^13.6.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.18.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "validate",
    "lint",
    "stylelint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}
